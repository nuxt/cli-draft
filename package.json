{
  "name": "nuxt-cli",
  "version": "0.0.1",
  "description": "CLI for Nuxt.js Projects",
  "keywords": [
    "nuxt",
    "nuxt.js",
    "nuxt cli"
  ],
  "repository": "nuxt/nuxt-cli",
  "license": "MIT",
  "bin": {
    "nuxt": "./bin/nuxt-cli.js",
    "nuxt-cli": "./bin/nuxt-cli.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "nuxt": "ts-node -e \"require('./src').bootstrap()\"",
    "build": "rollup -c",
    "lint": "eslint .",
    "test": "yarn lint"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^1.0.2",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/node": "^13.11.1",
    "eslint": "^6.8.0",
    "esm": "^3.2.25",
    "rollup": "^2.6.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
